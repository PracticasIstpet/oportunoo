<div class="container">
	<div class="sidebar  rounded-lg">

		<div class="white m-2 p-2 rounded-lg">
			<span class="font-weight-bolder">
				<i class=" fas fa-shopping-cart"></i>
				Transacción
			</span>
			<div class="d-flex justify-content-between">
				<span class="custom-control-inline ">Comprar</span>
				<div class="custom-control-inline">
					<label class="switch-long ">
						<input type="checkbox" {{#if transaccion.ischeck}} checked{{/if}}>
						<span class="slider-long "></span>
					</label>
				</div>
				<span class="custom-control-inline ">Arrendar</span>
			</div>

		</div>

		<div class="white m-2 p-2 rounded-lg ">
			<span class="font-weight-bolder">
				<i class=" fas fa-map-marked-alt "></i>
				Provincia
			</span>
			<div class="scroll-bar scrollbar-ripe-malinka" id="scroll-bar-countries">
				{{#each provincias}}
				<div class="custom-control custom-checkbox">
					<input type="checkbox" class="custom-control-input form-control" id="checks-provincias{{PROV_ID}}"
						name="checks-provincias" {{#if ischeck}} checked {{/if}}>
					<label class="custom-control-label" for="checks-provincias{{PROV_ID}}">{{PROV_NOMBRE}}</label>
				</div>
				{{#if ischeck}}
				<input type="hidden" id="check-focus-provincia" value="checks-provincias{{PROV_ID}}">
				<script>
					var id_provincia = document.getElementById("check-focus-provincia").value;
					document.getElementById(id_provincia).focus({ preventScroll: false });
				</script> {{/if}}
				{{/each}}
			</div>
		</div>

		<div class="white m-2 p-2 rounded-lg">
			<span class="font-weight-bolder">
